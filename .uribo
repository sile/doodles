{
  "update-readme": {
    "command": "fish",
    "args": [
      "-c",
      "set fish_trace 1; and cargo run --manifest-path genreadme/Cargo.toml -- --exclude-dir sketch > README.md; and cargo run --manifest-path genreadme/Cargo.toml -- --root-dir src/sketch/ > src/sketch/README.md; and git commit -am \"Update README.md\"; and git push origin main"
    ]
  }
}
